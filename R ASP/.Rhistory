createINd_cjo = function() {
m = matrix(c(rep(c(rep(0,237)),12)),nrow=237,byrow=TRUE)
for (j in 1:12) {
for(i in (j+1):237)
m[i,j]=mydata$IN_cjo[i-j]
}
INd_cjo = m[13:237,1:12] # on supprime les mois ou l'on a pas toutes les donnees a cause du decallage
return(INd_cjo)
}
